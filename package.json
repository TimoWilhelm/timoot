		"zod-validation-error": "^5.0.0"
	},
	"packageManager": "bun@1.3.5",
	"private": true,
	"scripts": {
		"prepare": "husky",
		"dev": "bun run build:worker-types && concurrently -n vite,types -c blue,yellow \"vite --host 0.0.0.0 --port 3000\" \"tsc -p tsconfig.worker.json --watch --preserveWatchOutput\"",
		"build": "turbo run build:vite",
		"build:worker-types": "tsc -p tsconfig.worker.json",
		"build:vite": "vite build",
		"typecheck": "turbo run typecheck:app typecheck:node typecheck:worker",
		"typecheck:app": "tsc --noEmit -p tsconfig.app.json",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json",
		"typecheck:worker": "tsc --noEmit -p tsconfig.worker.json",
		"lint": "prettier --check . && eslint --cache --format=stylish .",
		"format": "prettier --write . && eslint --cache --format=stylish --fix .",
		"test:unit": "vitest run --config test/unit/vitest.config.mts",
		"test:integration": "vitest run --config test/integration/vitest.config.mts",
		"test:react": "vitest run --config test/react/vitest.config.ts",
		"test:storybook": "vitest run --config test/storybook/vitest.config.ts",
		"test:e2e": "playwright test",
		"test:e2e:report": "playwright show-report",
		"preview": "turbo run build:vite && vite preview --host 0.0.0.0 --port 4173",
		"deploy": "turbo run build:vite && wrangler deploy",
		"cf-typegen": "wrangler types",
		"knip": "knip",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"type": "module"
}
