		"format": "prettier --write .",
		"test": "vitest run --config ./test/unit/vitest.config.mts",
		"test:watch": "vitest --config ./test/unit/vitest.config.mts",
		"test:integration": "vitest run --config ./test/integration/vitest.config.mts",
