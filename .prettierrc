{
	"printWidth": 140,
	"singleQuote": true,
	"semi": true,
	"useTabs": true,
	"trailingComma": "all",
	"overrides": [
		{
			"files": "*.md",
			"options": {
				"useTabs": false
			}
		},
		{
			"files": "*.jsonc",
			"options": {
				"trailingComma": "none"
			}
		}
	],
	"plugins": ["prettier-plugin-tailwindcss"]
}
